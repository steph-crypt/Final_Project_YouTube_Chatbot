    "from langchain_openai import OpenAIEmbeddings\n",
    "\n",
    "#initialize embedding model\n",
    "model_name = 'text-embedding-ada-002'\n",
    "\n",
    "embed = OpenAIEmbeddings(\n",
    "    model=model_name,\n",
    "    openai_api_key=OPENAI_API_KEY\n",
